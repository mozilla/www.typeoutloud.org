{
  "main": "server.js",
  "scripts": {
    "start": "npm-run-all --parallel watch:js server",
    "build:babel": "babel src --compact=true -s --out-dir dist",
    "build:webpack": "webpack --optimize-minimize --optimize-dedupe",
    "watch:js": "npm-run-all --parallel watch:babel watch:webpack",
    "watch:babel": "npm run build:babel -- -w",
    "watch:webpack": "webpack --progress --profile --colors --watch",
    "server": "node server.js",
    "postinstall": "npm-run-all build:*"
  },
  "author": "Mozilla",
  "license": "MPL-2.0",
  "engines": {
    "node": "^8.11.2"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.11.1",
    "body-parser": "^1.18.3",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "1.1.1",
    "eslint-plugin-react": "3.14.0",
    "express": "^4.16.3",
    "express-rate-limit": "^2.6.0",
    "extract-text-webpack-plugin": "0.9.1",
    "google-spreadsheet": "^2.0.4",
    "guid": "0.0.12",
    "habitat": "3.1.2",
    "helmet": "^3.5.0",
    "json-loader": "0.5.4",
    "less": "^3.0.4",
    "less-loader": "2.2.2",
    "lru-cache": "4.0.2",
    "normalize.css": "5.0.0",
    "npm-run-all": "1.4.0",
    "profam": "2.0.2",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-ga": "2.1.1",
    "react-router": "2.0.0",
    "request": "^2.86.0",
    "webpack": "1.12.9",
    "whatwg-fetch": "0.10.1"
  }
}
